<settings>
	<type>column</type>
	<add_time_stamp>1</add_time_stamp>
	<redraw>1</redraw>
	<depth>15</depth>
	<angle>15</angle>
	<colors>[#dd7b00,#80c02e,#127ed0,#14cfc4,#d9cf0e,#be2cd9,#oba986]</colors>     
	<text_color>000000</text_color>
  <background>
  	<color></color>
    <alpha>100</alpha>
    <border_alpha>20</border_alpha>
  </background>
  <plot_area>
    <margins>
      <left>60</left>
      <right>5</right>
      <top>40</top>
      <bottom>5</bottom>
    </margins>
  </plot_area>
  <grid>
    <category>
      <dashed>1</dashed>
    </category>
    <value>
      <dashed>1</dashed>
    </value>
  </grid>
  <values>                                          
    <category>                                  
      <enabled/>                                
      <frequency/>
      <start_from/>                            
      <rotate/>
      <color/>                                      
      <text_size/>                               
      <inside/>                                    
    </category>
    <value>                                         
      <enabled/>
      <reverse/>                                  
      <min/>                          
      <max/>                                        
      <strict_min_max/>                    
      <frequency/>                             
      <rotate/>                                    
      <skip_first/>                               
      <skip_last/>                                
      <color/>                                      
      <text_size/>                               
      <unit/>                                        
      <unit_position/>                       
      <integers_only/>                       
      <inside/>                                    
      <duration/>                                
    </value>
  </values>
  <axes>
    <category>
      <tick_length>1</tick_length>
      <width>1</width>
      <color>E7E7E7</color>
    </category>
    <value>
       <tick_length>1</tick_length>
      <width>1</width>
      <color>E7E7E7</color>
    </value>
  </axes>
  <column>
  	<type>clustered</type>
    <width>15</width>
    <balloon_text>{title}: {value}</balloon_text>
    <grow_time>3</grow_time>
  </column>
  <strings>
    <no_data>没有符合查询条件的记录</no_data>
  </strings>
  <graphs>
    <graph gid="1">
      <title></title>
      <type>line</type>
      <color>7F8DA9</color>
      <balloon_color>3352CC</balloon_color>
      <balloon_text>{series}-{title};{value}</balloon_text>
      <bullet>round</bullet>
      <bullet_size>4</bullet_size>
    </graph>
  </graphs>
  <labels>
    <label lid="0">
      <text><![CDATA[<b>标题</b>]]></text>
      <y>5</y>
      <text_color>000000</text_color>
      <text_size>12</text_size>
      <align>center</align>
    </label>
    <label lid="1">
      <text><![CDATA[]]></text>
      <x>1%</x>
      <y>2%</y>
    </label>
  </labels>
</settings>