<uicfg desc="数据导入配置按钮">
	<items>-></items>
	<items>
		<text>保存</text>
		<iconCls>save-icon</iconCls>
		<hidden type="object">this.isHidden()</hidden>
		<scope type="object">this.manager</scope>
		<handler type="object">function(){
			if(this.checkLine()) {
				this.editTbar.saveClick();
			}
		}</handler>
	</items>
	<items>
		<text>关闭</text>
		<iconCls>close-icon</iconCls>
		<handler type="object">this.closeClick</handler>
	</items>
</uicfg>
