<uicfg desc="栏目设置按钮">
	<items>-></items>
	<items>
		<text>新增</text>
		<iconCls>create-icon</iconCls>
		<scope type="object">this.manager</scope>
		<handler type="object">this.manager.addCatalog</handler>
	</items>
	<items>
		<text>删除</text>
		<iconCls>delete-selected-icon</iconCls>
		<scope type="object">this.manager</scope>
		<handler type="object">this.manager.delCatalog</handler>
	</items>
	<items>
		<text>保存</text>
		<iconCls>save-icon</iconCls>
		<scope type="object">this.manager</scope>
		<handler type="object">this.manager.saveSetting</handler>
	</items>
	<items>
		<text>关闭</text>
		<iconCls>close-icon</iconCls>
		<handler type="object">this.closeClick</handler>
	</items>
</uicfg>
