<uicfg desc="用户管理列表">
	<rownum type="boolean">true</rownum>
	<objectName>businessObject</objectName>
	<orderBy>code asc</orderBy>
	<editable type="object">true</editable>
	<formViewPath>system/example/user/listgridform</formViewPath>
	<store>
		<url>system/listUser.html</url>
		<fields>
			<name>id</name>
		</fields>
		<fields>
			<name>version</name>
		</fields>
		<fields>
			<name>userName</name>
		</fields>
		<fields>
			<name>loginName</name>
		</fields>
		<fields>
			<name>code</name>
		</fields>
		<fields>
			<name>sex</name>
		</fields>
		<fields>
			<name>birthday</name>
			<convert type="object">
				FW.convertDate
			</convert>
		</fields>
		<fields>
			<name>organization.id</name>
		</fields>
		<fields>
			<name>organization.name</name>
		</fields>
		<fields>
			<name>department.id</name>
		</fields>
		<fields>
			<name>department.name</name>
		</fields>
		<fields>
			<name>isEnabled</name>
		</fields>
		<fields>
			<name>isExpired</name>
		</fields>
		<fields>
			<name>isLocked</name>
		</fields>
		<fields>
			<name>isPortalUser</name>
		</fields>
	</store>
	<sm>
		<xtype>checkboxsm</xtype>
		<singleSelect type="boolean">false</singleSelect>
	</sm>
	<cm>
		<defaults>
			<sortable type="boolean">true</sortable>
		</defaults>
		<columns>
			<header>姓名</header>
			<dataIndex>userName</dataIndex>
			<editor>
				<xtype>textfield</xtype>
			</editor>
		</columns>
		<columns>
			<header>用户名</header>
			<dataIndex>loginName</dataIndex>
			<editor>
				<xtype>textfield</xtype>
				<showDetail type="boolean">true</showDetail>
			</editor>
		</columns>
		<columns>
			<header>工号</header>
			<dataIndex>code</dataIndex>
			<editor>
				<xtype>textfield</xtype>
			</editor>
		</columns>
		<columns>
			<header>性别</header>
			<dataIndex>sex</dataIndex>
			<keyType>sex</keyType>
			<editor>
				<xtype>lioncombobox</xtype>
				<keyType>sex</keyType>
			</editor>
		</columns>
		<columns>
			<header>生日</header>
			<dataIndex>birthday</dataIndex>
			<editor>
				<xtype>liondatefield</xtype>
				<format>Y-m-d H:i:s</format>
			</editor>
		</columns>
		<columns>
			<header>单位</header>
			<dataIndex>organization.name</dataIndex>
			<cascadeIndex>organization.id</cascadeIndex>
			<editor>
				<xtype>orgtree</xtype>
			</editor>
		</columns>
		<columns>
			<header>部门</header>
			<dataIndex>department.name</dataIndex>
			<cascadeIndex>department.id</cascadeIndex>
			<editor>
				<xtype>depttree</xtype>
			</editor>
		</columns>
		<columns>
			<header>是否启用</header>
			<dataIndex>isEnabled</dataIndex>
			<keyType>boolean</keyType>
		</columns>
		<columns>
			<header>是否过期</header>
			<dataIndex>isExpired</dataIndex>
			<keyType>boolean</keyType>
		</columns>
		<columns>
			<header>是否锁定</header>
			<dataIndex>isLocked</dataIndex>
			<keyType>boolean</keyType>
		</columns>
		<columns>
			<header>是否门户</header>
			<dataIndex>isPortalUser</dataIndex>
			<keyType>boolean</keyType>
		</columns>
	</cm>
</uicfg>
