	</items>
	<items>
		<xtype>liontextfield</xtype>
		<name>checkCode</name>
		<fieldLabel>考勤号</fieldLabel>
	</items>
	<items>
		<xtype>lioncombobox</xtype>
		<hiddenName>sex</hiddenName>
		<fieldLabel>性别</fieldLabel>
