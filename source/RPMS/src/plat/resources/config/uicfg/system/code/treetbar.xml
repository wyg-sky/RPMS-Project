	<items>
		<text>删除</text>
		<iconCls>delete-selected-icon</iconCls>
		<scope type="object">this.manager</scope>
		<handler type="object">this.manager.deleteCode</handler>
	</items>
	<items>
		<text>查看</text>
		<iconCls>view-icon</iconCls>
		<scope type="object">this.manager</scope>
		<handler type="object">this.manager.viewCode</handler>
