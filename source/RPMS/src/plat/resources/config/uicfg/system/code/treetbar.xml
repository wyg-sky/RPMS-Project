		<iconCls>create-icon</iconCls>
		<scope type="object">this.manager</scope>
		<handler type="object">this.manager.addCode</handler>
	</items>
	<items>
		<text>编辑</text>
		<iconCls>new-edit-icon</iconCls>
		<scope type="object">this.manager</scope>
		<handler type="object">this.manager.editCode</handler>
