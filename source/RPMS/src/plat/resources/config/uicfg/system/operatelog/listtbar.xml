<uicfg desc="操作日志管理按钮">
	<items>-></items>
	<items>
		<text>删除一年前的日志记录</text>
		<iconCls>delete-selected-icon</iconCls>
		<scope type="object">this.manager</scope>
		<handler type="object">this.manager.deleteLastYearLogs</handler>
	</items>
	<items>
		<text>删除一月前的日志记录</text>
		<iconCls>delete-selected-icon</iconCls>
		<scope type="object">this.manager</scope>
		<handler type="object">this.manager.deleteLastMonthLogs</handler>
	</items>
	<items>
		<text>清空日志记录</text>
		<iconCls>delete-selected-icon</iconCls>
		<scope type="object">this.manager</scope>
		<handler type="object">this.manager.deleteAllLogs</handler>
	</items>
	<items>
		<text>查询</text>
		<iconCls>search-d-icon</iconCls>
		<handler type="object">this.query</handler>
	</items>
	<items>
		<text>清空</text>
		<iconCls>ff-clear-icon</iconCls>
		<handler type="object">this.clear</handler>
	</items>
	<items>
		<iconCls>collapse-icon</iconCls>
		<tooltip>展开/收起查询区</tooltip>
		<handler type="object">this.expandQueryField</handler>
	</items>
</uicfg>
