<uicfg desc="单点链接管理列表">
	<rownum type="boolean">true</rownum>
	<objectName>businessObject</objectName>
	<orderBy>sort asc</orderBy>
	<addUrl>system/example/singlesystem/add.jsp</addUrl>
	<deleteUrl>system/deleteSingleSystem.html</deleteUrl>
	<editable type="object">true</editable>
	<formViewPath>system/example/singlesystem/listgridform</formViewPath>
	<store>
		<url>system/listSingleSystem.html</url>
		<fields>
			<name>id</name>
		</fields>
		<fields>
			<name>systemName</name>
		</fields>
		<fields>
			<name>systemType</name>
		</fields>
		<fields>
			<name>systemUrl</name>
		</fields>
		<fields>
			<name>systemEnable</name>
		</fields>
		<fields>
			<name>sort</name>
		</fields>
		<fields>
			<name>systemUserVariable</name>
		</fields>
		<fields>
			<name>systemPwdVariable</name>
		</fields>
		<fields>
			<name>createUser.userName</name>
		</fields>
		<fields>
			<name>createTime</name>
		</fields>
	</store>
	<sm>
		<xtype>checkboxsm</xtype>
		<singleSelect type="boolean">false</singleSelect>
	</sm>
	<cm>
		<defaults>
			<sortable type="boolean">true</sortable>
		</defaults>
		<columns>
			<header>系统名称</header>
			<dataIndex>systemName</dataIndex>
		</columns>
		<columns>
			<header>系统类型</header>
			<dataIndex>systemType</dataIndex>
			<keyType>system_single_type</keyType>
		</columns>
		<columns>
			<header>系统地址</header>
			<dataIndex>systemUrl</dataIndex>
		</columns>
		<columns>
			<header>输出顺序</header>
			<dataIndex>sort</dataIndex>
		</columns>
		<columns>
			<header>是否启用</header>
			<dataIndex>systemEnable</dataIndex>
			<keyType>boolean</keyType>
		</columns>
		<columns>
			<header>创建人</header>
			<dataIndex>createUser.userName</dataIndex>
		</columns>
		<columns>
			<header>创建时间</header>
			<dataIndex>createTime</dataIndex>
		</columns>
	</cm>
</uicfg>