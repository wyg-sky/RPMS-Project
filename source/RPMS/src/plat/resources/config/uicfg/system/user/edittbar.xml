<uicfg desc="表单编辑按钮">
	<items>-></items>
	<items>
		<text>保存</text>
		<iconCls>save-icon</iconCls>
		<hidden type="object">this.manager.userInfo?false:!this.manager.editable</hidden>
		<handler>
			<![CDATA[
			function(){
				this.saveClick();
			}]]>
		</handler>
	</items>
	<items>
		<text>打印</text>
		<iconCls>print-icon</iconCls>
		<handler type="object">this.printForm</handler>
	</items>
	<items>
		<text>关闭</text>
		<iconCls>close-icon</iconCls>
		<handler type="object">this.closeClick</handler>
	</items>
</uicfg>
