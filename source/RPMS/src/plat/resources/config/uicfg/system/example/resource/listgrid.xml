<uicfg desc="资源管理列表">
	<rownum type="boolean">true</rownum>
	<objectName>businessObject</objectName>
	<orderBy>id asc</orderBy>
	<formViewPath>system/example/resource/listgridform</formViewPath>
	<store>
		<url>system/listResource.html</url>
		<fields>
			<name>id</name>
		</fields>
		<fields>
			<name>name</name>
		</fields>
		<fields>
			<name>xaction</name>
		</fields>
		<fields>
			<name>resourceType</name>
		</fields>
		<fields>
			<name>operType</name>
		</fields>
		<fields>
			<name>description</name>
		</fields>
		<fields>
			<name>Module</name>
		</fields>
		<fields>
			<name>organization.id</name>
		</fields>
		<fields>
			<name>organization.name</name>
		</fields>
		<fields>
			<name>department.id</name>
		</fields>
		<fields>
			<name>department.name</name>
		</fields>
	</store>
	<sm>
		<xtype>checkboxsm</xtype>
		<singleSelect type="boolean">false</singleSelect>
	</sm>
	<cm>
		<defaults>
			<sortable type="boolean">true</sortable>
		</defaults>
		<columns>
			<header>资源名称</header>
			<dataIndex>name</dataIndex>			
		</columns>
		<columns>
			<header>资源类型</header>
			<dataIndex>resourceType</dataIndex>
		</columns>
		<columns>
			<header>操作类型</header>
			<dataIndex>operType</dataIndex>
		</columns>
		<columns>
			<header>资源编码</header>
			<dataIndex>xaction</dataIndex>
		</columns>
		<columns>
			<header>所属模块</header>
			<dataIndex>Module</dataIndex>
		</columns>
		<columns>
			<header>资源描述</header>
			<dataIndex>description</dataIndex>
		</columns>
	</cm>
</uicfg>
