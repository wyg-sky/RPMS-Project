<uicfg desc="组件参数管理按钮">
	<items>-></items>
	<items>
		<text>新增自定义参数</text>
		<iconCls>new-new-icon</iconCls>
		<handler type="object">function(){
			this.manager.addLine(this.manager.barparamsGrid);
		}</handler>
	</items>
	<items>
		<text>新增</text>
		<iconCls>new-new-icon</iconCls>
		<handler type="object">function(){
			this.manager.addLineObject(this.manager.barparamsGrid, null, '0006', null, null, null);
		}</handler>
	</items>
	<items>
		<text>删除</text>
		<iconCls>delete-selected-icon</iconCls>
		<handler type="object">function(){
			this.manager.deleteObject(this.manager.barparamsGrid);
		}</handler>
	</items>
	<items>
		<text>保存</text>
		<iconCls>search-d-icon</iconCls>
		<handler type="object">function() {
			this.manager.widgetSave(this.manager.tbarsGrid, this.manager.barparamsGrid);
		}</handler>
	</items>
</uicfg>
