<uicfg desc="组件参数管理按钮">
	<items>-></items>
	<items>
		<text>新增</text>
		<iconCls>new-new-icon</iconCls>
		<handler type="object">function(){
			this.manager.addLine(this.manager.tbarsGrid);
		}</handler>
	</items>
	<items>
		<text>删除</text>
		<iconCls>delete-selected-icon</iconCls>
		<handler type="object">function(){
			this.manager.deleteObject(this.manager.tbarsGrid);
		}</handler>
	</items>
	<items>
		<text>完成</text>
		<iconCls>save-icon</iconCls>
		<handler type="object">function(){
			this.manager.generateTbar();
		}</handler>
	</items>
	<items>
		<text>关闭</text>
		<iconCls>close-icon</iconCls>
		<handler type="object">this.closeClick</handler>
	</items>
</uicfg>
