	<changeStatusUrl>base/changeLocationStatus.html</changeStatusUrl>
	<component>
		<items>
			<object>dataGrid</object>
			<xtype>liontreegrid</xtype>
			<viewPath>base/location/listgrid</viewPath>
		</items>
		<items>
			<object>queryForm</object>
			<xtype>lionform</xtype>
			<viewPath>base/location/listqueryform</viewPath>
			<runMode>4</runMode>
