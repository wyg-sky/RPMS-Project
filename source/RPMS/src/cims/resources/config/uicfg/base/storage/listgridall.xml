<uicfg desc="仓库基本信息列表">
	<rownum type="boolean">true</rownum>
	<objectName>businessObject</objectName>
	<orderBy>storageCd asc</orderBy>
	<editable type="object">false</editable>
	<showCopyButton type="object">true</showCopyButton>
	<statusCtrl>
		<field>valid</field>
		<value>1</value>
		<text>有效</text>
	</statusCtrl>
	<store>
		<url>base/listStorageAll.html</url>
		<fields>
			<name>id</name>
		</fields>
		<fields>
			<name>storageCd</name>
		</fields>
		<fields>
			<name>storageName</name>
		</fields>
		<fields>
			<name>locationName</name>
		</fields>
		<fields>
			<name>storageType</name>
		</fields>
		<fields>
			<name>capitalLimit</name>
		</fields>
		<fields>
			<name>area</name>
		</fields>
		<fields>
			<name>organization.id</name>
		</fields>
		<fields>
			<name>organization.name</name>
		</fields>
		<fields>
			<name>valid</name>
		</fields>
	</store>
	<sm>
		<xtype>checkboxsm</xtype>
		<singleSelect type="boolean">false</singleSelect>
	</sm>
	<cm>
		<defaults>
			<sortable type="boolean">true</sortable>
		</defaults>
		<columns>
			<header>仓库编号</header>
			<dataIndex>storageCd</dataIndex>
		</columns>
		<columns>
			<header>仓库名称</header>
			<dataIndex>storageName</dataIndex>
		</columns>
		<columns>
			<header>仓库地点</header>
			<dataIndex>locationName</dataIndex>
		</columns>
		<columns>
			<header>仓库类型</header>
			<dataIndex>storageType</dataIndex>
			<keyType>equi_storage_type</keyType>
		</columns>
		<columns>
			<header>所属单位</header>
			<dataIndex>organization.name</dataIndex>
		</columns>
		<columns>
			<header>是否有效</header>
			<dataIndex>valid</dataIndex>
			<keyType>cims_effective</keyType>
		</columns>
	</cm>
</uicfg>
