	</items>
	<items>
		<xtype>lioncodefield</xtype>
		<name>faceCd</name>
		<fieldLabel>工作面编号</fieldLabel>
		<readOnly type="boolean">true</readOnly>
	</items>
	<items>
		<xtype>liontextfield</xtype>
		<name>faceName</name>
		<fieldLabel>工作面名称</fieldLabel>
