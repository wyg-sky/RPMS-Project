<uicfg desc="通知明细表单编辑按钮">
	<items>-></items>
	<items>
		<text>保存</text>
		<iconCls>save-icon</iconCls>
		<hidden type="object">this.manager.buttHid == true?false:true</hidden>
		<handler type="object">function() {
				this.manager.saveLineObject();
		}
		</handler>
	</items>
	<items>
		<text>关闭</text>
		<iconCls>close-icon</iconCls>
		<handler type="object">this.closeClick</handler>
	</items>
</uicfg>
