	<winWidth type="integer">850</winWidth>
	<component>
		<items>
			<object>editTbar</object>
			<xtype>liontoolbar</xtype>
			<viewPath>rpms/innovate/innovateinstitution/edittbar</viewPath>
		</items>
		<items>
			<object>editForm</object>
			<xtype>lionform</xtype>
			<viewPath>rpms/innovate/innovateinstitution/editform</viewPath>
		</items>
		<items>
			<object>fileLineGrid</object>
