<uicfg desc="专利申请管理列表">
	<rownum type="boolean">true</rownum>
	<objectName>businessObject</objectName>
	<orderBy></orderBy>
	<editable type="object">false</editable>
	<showCopyButton type="object">false</showCopyButton>
	<statusCtrl>
		<field>status</field>
		<value>0002,0003,0005,0006</value>
		<text>已上报或审批通过</text>
	</statusCtrl>
	<store>
		<url>rpms/listPatentApplication.html</url>
		<fields>
			<name>id</name>
		</fields>
		<fields>
			<name>patentName</name>
		</fields>
		<fields>
			<name>patentNum</name>
		</fields>
		<fields>
			<name>organization.name</name>
		</fields>
		<fields>
            <name>platCenter.platName</name>
        </fields>
		<fields>
			<name>platInstitution.id</name>
		</fields>
		<fields>
            <name>platInstitution.platName</name>
        </fields>
        <fields>
			<name>specialty.specialtyName</name>
		</fields>
		<fields>
			<name>business.specialtyName</name>
		</fields>
		<fields>
			<name>invnetor</name>
		</fields>
        <fields>
			<name>type</name>
		</fields>
		
		<fields>
			<name>applyDate</name>
		</fields>
		<fields>
			<name>acceptDate</name>
		</fields>
		<fields>
			<name>publicExpense</name>
		</fields>
		<fields>
			<name>applicationFee</name>
		</fields>
		<fields>
			<name>privilegeDate</name>
		</fields>
		<fields>
			<name>regFee</name>
		</fields>
		<fields>
			<name>annualFee</name>
		</fields>
		<fields>
			<name>taxes</name>
		</fields>
		<fields>
			<name>protectDate</name>
		</fields>
		<fields>
			<name>spreadType</name>
		</fields>
		<fields>
			<name>abstracts</name>
		</fields>
		<fields>
			<name>remark</name>
		</fields>
		<fields>
			<name>patentee</name>
		</fields>
		<fields>
			<name>patenteeAddress</name>
		</fields>
		<fields>
			<name>technosphere</name>
		</fields>
		<fields>
			<name>status</name>
		</fields>
		<fields>
			<name>num</name>
		</fields>
		<fields>
			<name>valid</name>
		</fields>
		<fields>
			<name>reportUser</name>
		</fields>
		<fields>
			<name>reportTime</name>
		</fields>
		<fields>
			<name>examineUser</name>
		</fields>
		<fields>
			<name>examineSug</name>
		</fields>
		<fields>
			<name>approveUser</name>
		</fields>
		<fields>
			<name>approveSug</name>
		</fields>
		
	</store>
	<sm>
		<xtype>checkboxsm</xtype>
		<singleSelect type="boolean">false</singleSelect>
	</sm>
	<cm>
		<defaults>
			<sortable type="boolean">true</sortable>
		</defaults>
		<columns>
			<header>专利名称</header>
			<dataIndex>patentName</dataIndex>
			<width type="object">250</width>
		</columns>
		<columns>
			<header>专利号</header>
			<dataIndex>patentNum</dataIndex>
		</columns>
		<columns>
			<header>单位</header>
			<dataIndex>organization.name</dataIndex>
			<width type="object">150</width>
		</columns>
		<columns>
			<header>分中心</header>
			<dataIndex>platCenter.platName</dataIndex>
			<width type="object">150</width>
		</columns>
		<columns>
			<header>分中心机构</header>
			<dataIndex>platInstitution.platName</dataIndex>
			<width type="object">150</width>
		</columns>
		<columns>
			<header>专业</header>
			<dataIndex>specialty.specialtyName</dataIndex>
		</columns>
		<columns>
			<header>业务</header>
			<dataIndex>business.specialtyName</dataIndex>
		</columns>
		<columns>
			<header>发明人</header>
			<dataIndex>invnetor</dataIndex>
			<width type="object">250</width>
		</columns>
		<columns>
			<header>专利分类</header>
			<keyType>rpms_patent_type</keyType>
			<dataIndex>type</dataIndex>
		</columns>
		<columns>
			<header>申请日期</header>
			<dataIndex>applyDate</dataIndex>
		</columns> 
		<columns>
			<header>受理日期</header>
			<dataIndex>acceptDate</dataIndex>
		</columns>
		<columns>
			<header>授权日期</header>
			<dataIndex>privilegeDate</dataIndex>
		</columns>
		<columns>
			<header>保护到期时间</header>
			<dataIndex>protectDate</dataIndex>
		</columns>
		<columns>
			<header>推广类别</header>
			<dataIndex>spreadType</dataIndex>
			<keyType>rpms_promotion_category</keyType>
		</columns>
		<columns>
			<header>专利说明</header>
			<dataIndex>abstracts</dataIndex>
		</columns>
		<columns>
			<header>备注</header>
			<dataIndex>remark</dataIndex>
		</columns>
	</cm>
</uicfg>
