<uicfg desc="项目进度阶段按钮">
    <items>-></items>
    <items>
        <text>新增</text>
        <iconCls>new-new-icon</iconCls>
        <handler type="object">
            function() {
                this.manager.addStages();
            }
        </handler>
    </items>
    <items>
        <text>删除</text>
        <iconCls>delete-selected-icon</iconCls>
        <handler type="object">
            function() {
                this.manager.deleteStages();
            }
        </handler>
    </items>
</uicfg>
