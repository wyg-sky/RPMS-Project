<uicfg desc="合作协作单位明细按钮">
    <items>-></items>
    <items>
        <text>新增</text>
        <iconCls>new-new-icon</iconCls>
        <handler type="object">
            function() {
                this.manager.addPartner();
            }
        </handler>
    </items>
    <items>
        <text>删除</text>
        <iconCls>delete-selected-icon</iconCls>
        <handler type="object">
            function() {
                this.manager.deletePartner();
            }
        </handler>
    </items>
</uicfg>
