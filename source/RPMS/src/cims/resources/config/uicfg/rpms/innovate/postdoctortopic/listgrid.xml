<uicfg desc="博士后课题管理列表">
	<rownum type="boolean">true</rownum>
	<objectName>businessObject</objectName>
	<orderBy></orderBy>
	<editable type="object">false</editable>
	<showCopyButton type="object">false</showCopyButton>
	<store>
		<url>rpms/listPostdoctorTopic.html</url>
		<fields>
			<name>id</name>
		</fields>
		<fields>
			<name>platCenter.id</name>
		</fields>
		<fields>
            <name>platCenter.platName</name>
        </fields>
		<fields>
			<name>platInstitution.id</name>
		</fields>
		<fields>
            <name>platInstitution.platName</name>
        </fields>
		<fields>
			<name>topic</name>
		</fields>
		<fields>
			<name>startDate</name>
		</fields>
		<fields>
			<name>endDate</name>
		</fields>
		<fields>
			<name>talent.id</name>
		</fields>
		<fields>
			<name>talent.talentName</name>
		</fields>
		<fields>
			<name>state</name>
		</fields>
		<fields>
			<name>inDate</name>
		</fields>
		<fields>
			<name>outDate</name>
		</fields>
	</store>
	<sm>
		<xtype>checkboxsm</xtype>
		<singleSelect type="boolean">false</singleSelect>
	</sm>
	<cm>
		<defaults>
			<sortable type="boolean">true</sortable>
		</defaults>
		<columns>
			<header>分中心</header>
			<dataIndex>platCenter.platName</dataIndex>
		</columns>
		<columns>
			<header>分中心机构</header>
			<dataIndex>platInstitution.platName</dataIndex>
		</columns>
		<columns>
			<header>课题名称</header>
			<dataIndex>topic</dataIndex>
			<width type="object">250</width>		
		</columns>
		<columns>
			<header>开始日期</header>
			<dataIndex>startDate</dataIndex>
		</columns>
		<columns>
			<header>结束日期</header>
			<dataIndex>endDate</dataIndex>
		</columns>
		<columns>
			<header>博士后姓名</header>
			<dataIndex>talent.talentName</dataIndex>
		</columns>
		<columns>
			<header>状态</header>
			<dataIndex>state</dataIndex>
			<keyType>rpms_postdoctor_status</keyType>
		</columns>
		<columns>
			<header>进站时间</header>
			<dataIndex>inDate</dataIndex>
		</columns>
		<columns>
			<header>出站时间</header>
			<dataIndex>outDate</dataIndex>
		</columns>
	</cm>
</uicfg>
