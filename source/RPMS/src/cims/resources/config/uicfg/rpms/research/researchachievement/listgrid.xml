<uicfg desc="成果管理列表">
	<rownum type="boolean">true</rownum>
	<objectName>businessObject</objectName>
	<orderBy>certificationDate desc</orderBy>
	<editable type="object">false</editable>
    <showEditButton type="object">Ext.isAllowed('rpms_researchachievement_edit')</showEditButton>
	<showCopyButton type="object">false</showCopyButton>
	<store>
		<url>rpms/listResearchAchievement.html</url>
		<fields>
			<name>id</name>
		</fields>
		<fields>
			<name>achievementName</name>
		</fields>
		<fields>
			<name>achievementNum</name>
		</fields>
		<fields>
			<name>achievementLevel</name>
		</fields>
		<fields>
			<name>organization.id</name>
		</fields>
		<fields>
			<name>organization.name</name>
		</fields>
		<fields>
			<name>platCenter.id</name>
		</fields>
		<fields>
			<name>platCenter.platName</name>
		</fields>
		<fields>
			<name>platInstitution.id</name>
		</fields>
		<fields>
			<name>platInstitution.platName</name>
		</fields>
		<fields>
			<name>projectId.id</name>
		</fields>
		<fields>
			<name>projectId.projectName</name>
		</fields>
		<fields>
			<name>specialty.id</name>
		</fields>
		<fields>
			<name>specialty.specialtyName</name>
		</fields>
		<fields>
			<name>type</name>
		</fields>
		<fields>
			<name>reviewMechanism</name>
		</fields>
		<fields>
			<name>certificationDate</name>
		</fields>
	</store>
	<sm>
		<xtype>checkboxsm</xtype>
		<singleSelect type="boolean">false</singleSelect>
	</sm>
	<cm>
		<defaults>
			<sortable type="boolean">true</sortable>
		</defaults>
		<columns>
            <header>成果名称</header>
            <dataIndex>achievementName</dataIndex>
        </columns>
        <columns>
            <header>成果编号</header>
            <dataIndex>achievementNum</dataIndex>
        </columns>
        <columns>
            <header>单位</header>
            <dataIndex>organization.name</dataIndex>
        </columns>
		<columns>
			<header>分中心</header>
			<dataIndex>platCenter.platName</dataIndex>
		</columns>
		<columns>
			<header>分中心机构</header>
			<dataIndex>platInstitution.platName</dataIndex>
		</columns>
		<columns>
            <header>专业</header>
            <dataIndex>specialty.specialtyName</dataIndex>
        </columns>
        <columns>
            <header>成果类型</header>
            <dataIndex>type</dataIndex>
            <keyType>rpms_acceptance_type</keyType>
        </columns>
		<columns>
            <header>评议鉴定机构</header>
            <dataIndex>reviewMechanism</dataIndex>
            <keyType>rpms_review_unit</keyType>
        </columns>
		<columns>
			<header>评议(鉴定)时间</header>
			<dataIndex>certificationDate</dataIndex>
		</columns>
	</cm>
</uicfg>