<uicfg desc="项目资金明细按钮">
    <items>-></items>
    <items>
        <text>新增</text>
        <iconCls>new-new-icon</iconCls>
        <handler type="object">
            function() {
                this.manager.addFunds();
            }
        </handler>
    </items>
    <items>
        <text>删除</text>
        <iconCls>delete-selected-icon</iconCls>
        <handler type="object">
            function() {
                this.manager.deleteFunds();
            }
        </handler>
    </items>
</uicfg>
