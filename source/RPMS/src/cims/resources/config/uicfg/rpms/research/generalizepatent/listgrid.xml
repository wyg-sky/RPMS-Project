<uicfg desc="专利列表">
	<rownum type="boolean">true</rownum>
	<objectName>businessObject</objectName>
	<orderBy>privilegeDate desc,protectDate desc</orderBy>
	<editable type="object">false</editable>
    <showEditButton type="object">Ext.isAllowed('rpms_researchpatent_edit')</showEditButton>
	<showCopyButton type="object">false</showCopyButton>
	<store>
		<url>rpms/listResearchPatent.html</url>
		<autoLoad type="object">true</autoLoad>
		<baseParams>
			<hql type="object">" and(obj.spreadType is null or obj.spreadType is not '0003')"</hql>
		</baseParams>
		<fields>
			<name>id</name>
		</fields>
		<fields>
			<name>organization.name</name>
		</fields>
		<fields>
			<name>platCenter.id</name>
		</fields>
		<fields>
            <name>platCenter.platName</name>
        </fields>
		<fields>
			<name>platInstitution.id</name>
		</fields>
		<fields>
            <name>platInstitution.platName</name>
        </fields>
		<fields>
			<name>linkman</name>
		</fields>
		<fields>
			<name>phonenum</name>
		</fields>
		<fields>
			<name>postcode</name>
		</fields>
		<fields>
			<name>email</name>
		</fields>
		<fields>
			<name>applyDate</name>
		</fields>
		<fields>
			<name>patentName</name>
		</fields>
		<fields>
			<name>firstInventorName</name>
		</fields>
		<fields>
			<name>firstInventorPhonenum</name>
		</fields>
		<fields>
			<name>patentAnent</name>
		</fields>
		<fields>
			<name>type</name>
		</fields>
		<fields>
			<name>projectId</name>
		</fields>
		<fields>
			<name>patentNum</name>
		</fields>
		<fields>
			<name>acceptDate</name>
		</fields>
		<fields>
			<name>privilegeDate</name>
		</fields>
		<fields>
			<name>protectDate</name>
		</fields>
		<fields>
			<name>invnetor</name>
		</fields>
		<fields>
			<name>patentee</name>
		</fields>
		<fields>
			<name>patenteeAddress</name>
		</fields>
		<fields>
			<name>technosphere</name>
		</fields>
		<fields>
			<name>abstracts</name>
		</fields>
		<fields>
			<name>status</name>
		</fields>
		<fields>
			<name>num</name>
		</fields>
		<fields>
			<name>valid</name>
		</fields>
		<fields>
			<name>reportUser</name>
		</fields>
		<fields>
			<name>reportTime</name>
		</fields>
		<fields>
			<name>examineUser</name>
		</fields>
		<fields>
			<name>examineSug</name>
		</fields>
		<fields>
			<name>approveUser</name>
		</fields>
		<fields>
			<name>approveSug</name>
		</fields>
		<fields>
			<name>specialty.specialtyName</name>
		</fields>
		<fields>
			<name>protectedAlert</name>
		</fields>
		<fields>
			<name>spreadType</name>
		</fields>
	</store>
	<sm>
		<xtype>checkboxsm</xtype>
		<singleSelect type="boolean">false</singleSelect>
	</sm>
	<cm>
		<defaults>
			<sortable type="boolean">true</sortable>
		</defaults>
		<columns>
			<header>专利名称</header>
			<dataIndex>patentName</dataIndex>
			<width type="object">250</width>
		</columns>
		<columns>
			<header>专利号</header>
			<dataIndex>patentNum</dataIndex>
			<width type="object">150</width>
		</columns>
		<columns>
			<header>单位</header>
			<dataIndex>organization.name</dataIndex>
			<width type="object">150</width>
		</columns>
		<columns>
			<header>分中心</header>
			<dataIndex>platCenter.platName</dataIndex>
		</columns>
		<columns>
			<header>分中心机构</header>
			<dataIndex>platInstitution.platName</dataIndex>
		</columns>
		<columns>
			<header>专业</header>
			<dataIndex>specialty.specialtyName</dataIndex>
		</columns>
		<columns>
			<header>发明人</header>
			<dataIndex>invnetor</dataIndex>
			<width type="object">250</width>
		</columns>
		<columns>
			<header>分类</header>
			<keyType>rpms_patent_type</keyType>
			<dataIndex>type</dataIndex>
		</columns>
		<columns>
			<header>授权日期</header>
			<dataIndex>privilegeDate</dataIndex>
		</columns>
		<columns>
			<header>保护到期日期</header>
			<dataIndex>protectDate</dataIndex>
		</columns>
		<columns>
			<header>推广类别</header>
			<dataIndex>spreadType</dataIndex>
			<keyType>rpms_promotion_category</keyType>
		</columns>
	</cm>
</uicfg>