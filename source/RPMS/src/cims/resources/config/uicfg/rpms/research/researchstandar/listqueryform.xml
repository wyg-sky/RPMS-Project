<uicfg desc="标准管理查询表单">
	<columns type="integer">4</columns>
	<autoHeight type="boolean">true</autoHeight>
	<defaultFunction type="object">this.manager.query</defaultFunction>
	<defaultScope type="object">this.manager</defaultScope>
	<items>
		<xtype>lionblankfield</xtype>
	</items>
	<items>
		<xtype>liontextfield</xtype>
		<fieldLabel>标准名称</fieldLabel>
		<name>standarName</name>
	</items>
	<items>
		<xtype>liontextfield</xtype>
		<fieldLabel>标准号</fieldLabel>
		<name>standarNum</name>
	</items>
    <items>
        <xtype>lioncombobox</xtype>
        <fieldLabel>标准状态</fieldLabel>
        <name>standarStatus</name>
        <keyType>rpms_standar_status</keyType>
    </items>
</uicfg>
