	<head>
		<property name="type">0</property>
		<name><![CDATA[RPMS_Entpri_Science_Activity_Info]]></name>
		<dataSets>
			<dataSet name="sum" type="DB_SQL" >
				<property name="dataSource">CIMS_JSZX</property>
				<property name="desc"><![CDATA[企业科技活动及相关情况]]></property>
				<property name="query"><![CDATA[select *
  from (select nvl(sum(a.BUSINESS_INCOME), 0) as main_income_sum,
               nvl(sum(to_number(nvl(a.pseson_cost, 0)) +
                       to_number(nvl(a.scidenc_service, 0)) +
                       to_number(nvl(a.work_fee, 0)) +
                       to_number(nvl(a.design_fee, 0)) +
                       to_number(nvl(a.degugging_fee, 0)) +
                       to_number(nvl(a.material_fee, 0)) +
                       to_number(nvl(a.gas_electricity, 0)) +
                       to_number(nvl(a.moule_exam, 0)) +
                       to_number(nvl(a.instrument_equi, 0)) +
                       to_number(nvl(a.use_building, 0)) +
                       to_number(nvl(a.equi_rental, 0)) +
                       to_number(nvl(a.faclities_fee, 0)) +
                       to_number(nvl(a.technology_fee, 0)) +
                       to_number(nvl(a.office_fee, 0))),
                   0) as pro_innercost_sum,
               nvl(sum(to_number(nvl(a.pseson_cost, 0)) +
                       to_number(nvl(a.scidenc_service, 0)) +
                       to_number(nvl(a.work_fee, 0))),
                   0) as pseson_cost_sum,
               nvl(sum(to_number(nvl(a.material_fee, 0)) +
                       to_number(nvl(a.gas_electricity, 0)) +
                       to_number(nvl(a.moule_exam, 0))),
                   0) as material_cost_sum,
               nvl(sum(to_number(nvl(a.instrument_equi, 0)) +
                       to_number(nvl(a.use_building, 0)) +
                       to_number(nvl(a.equi_rental, 0)) +
                       to_number(nvl(a.faclities_fee, 0))),
                   0) as depreciation_sum,
               
               nvl(sum(to_number(nvl(a.technology_fee, 0))), 0) as intangible_cost_sum,
               
               nvl(sum(to_number(nvl(a.office_fee, 0))), 0) as other_cost_sum,
               
               nvl(sum(to_number(nvl(a.enterprice_fee, 0)) +
                       to_number(nvl(a.school_fee, 0)) +
                       to_number(nvl(a.abroad_fee, 0))),
                   0) as pro_outercost_sum,
               nvl(sum(to_number(nvl(a.enterprice_fee, 0))), 0) as enterprice_fee_sum,
               nvl(sum(to_number(nvl(a.school_fee, 0))), 0) as school_fee_sum,
               nvl(sum(to_number(nvl(a.abroad_fee, 0))), 0) as abroad_fee_sum,
               
               nvl(sum(to_number(nvl(a.equi_price, 0)) +
                       to_number(nvl(a.building_price, 0))),
                   0) as fixed_assets_sum,
               
               nvl(sum(to_number(nvl(a.equi_price, 0))), 0) as equi_price_sum,
               
               nvl(sum(to_number(nvl(a.government_funds, 0))), 0) as government_funds_sum,
               
               count(a.project_id) as count_proid_num,
               nvl(sum(to_number(nvl(a.pseson_cost, 0)) +
                       to_number(nvl(a.scidenc_service, 0)) +
                       to_number(nvl(a.work_fee, 0)) +
                       to_number(nvl(a.design_fee, 0)) +
                       to_number(nvl(a.degugging_fee, 0)) +
                       to_number(nvl(a.material_fee, 0)) +
                       to_number(nvl(a.gas_electricity, 0)) +
                       to_number(nvl(a.moule_exam, 0)) +
                       to_number(nvl(a.instrument_equi, 0)) +
                       to_number(nvl(a.use_building, 0)) +
                       to_number(nvl(a.equi_rental, 0)) +
                       to_number(nvl(a.faclities_fee, 0)) +
                       to_number(nvl(a.technology_fee, 0)) +
                       to_number(nvl(a.office_fee, 0))),
                   0) as allpro_innercost_sum
          from (select id,
                       project_id,
                       dept             as platcId,
                       report_time      as platinId,
                       years            as yearMonth,
                       business_income,
                       pseson_cost,
                       scidenc_service,
                       work_fee,
                       design_fee,
                       degugging_fee,
                       material_fee,
                       gas_electricity,
                       moule_exam,
                       instrument_equi,
                       use_building,
                       equi_rental,
                       faclities_fee,
                       technology_fee,
                       office_fee,
                       enterprice_fee,
                       school_fee,
                       abroad_fee,
                       equi_price,
                       government_funds,
                       count_person,
                       report_person,
                       phone,
                       building_price
                  from rpms_science_activity) a
         where 1 = 1 ${con}),
       
       (select count(id) project_talent_num,
               count(CASE
                       WHEN sex IN ('0001') THEN
                        '1'
                     END) sex_num,
               count(CASE
                       WHEN title IN ('0001', '0002', '0003', '0004', '0005') THEN
                        '1'
                     END) title_num
          from (select *
                  from rpms_research_talent
                 where id in
                       (select t.research_talent
                          from RPMS_PROJECT_TALENT t
                         where main_id in (select id
                                             from (select p.id,
                                                          p.plat_center      as platcId,
                                                          p.plat_institution as platinId,
                                                          p.report_time      as yearMonth
                                                     from RPMS_PROJECT p)
                                            where 1 = 1 ${con})))
        
        ),
       
       (select count(CASE
                       WHEN status in ('0003', '0005', '0006') THEN
                        '1'
                     END) zlsb_num,
               count(CASE
                       WHEN status in ('0003', '0005', '0006') and TYPE = '0001' THEN
                        '1'
                     END) fmzlsb_num
          from (select zlsb.id               as id,
                       zlsb.plat_center      as platcId,
                       zlsb.plat_institution as platinId,
                       zlsb.apply_date       as yearMonth,
                       zlsb.type             as type,
                       zlsb.status           as status
                  from RPMS_PATENT_APPLICATION zlsb)
         where 1 = 1 ${con}),
       
       (select count(CASE
                       WHEN status in ('0006') THEN
                        '1'
                     END) zlsq_num,
               count(CASE
                       WHEN status in ('0006') and TYPE = '0001' THEN
                        '1'
                     END) fmzlsq_num
          from (select zlsq.id               as id,
                       zlsq.plat_center      as platcId,
                       zlsq.plat_institution as platinId,
                       zlsq.privilege_date   as yearMonth,
                       zlsq.type             as type,
                       zlsq.status           as status
                  from RPMS_PATENT_APPLICATION zlsq)
         where 1 = 1 ${con}),
       
       (select count(id) as lwfb_num
          from (select id,
                       plat_center      as platcId,
                       plat_institution as platinId,
                       pubtime          as yearMonth
                  from RPMS_RESEARCH_THESIS)
         where 1 = 1 ${con}),
       
       (select count(id) as standar_num
          from (select id,
                       plat_center      as platcId,
                       plat_institution as platinId,
                       realease_date    as yearMonth
                  from RPMS_RESEARCH_STANDAR
                 where grade in ('0001', '0002'))
         where 1 = 1 ${con}),
       
       (select count(id) as platin_num
          from (select id from rpms_research_plat p where 1 = 1 ${con1})),
       
       (select count(id) as plat_talent_num,
               count(case
                       when education = '0001' then
                        '1'
                     end) as dr_num,
               count(case
                       when education = '0002' then
                        '1'
                     end) as mr_num,
               count(case
                       when education = '0003' then
                        '1'
                     end) as sr_num
          from (select *
                  from rpms_research_talent t
                 where t.plat_institution in
                       ((select id
                          from rpms_research_plat p
                         where 1 = 1 ${con1})))),
       
       (select sum(to_number(nvl(pseson_cost, 0)) +
                   to_number(nvl(scidenc_service, 0)) +
                   to_number(nvl(work_fee, 0)) +
                   to_number(nvl(design_fee, 0)) +
                   to_number(nvl(degugging_fee, 0)) +
                   to_number(nvl(material_fee, 0)) +
                   to_number(nvl(gas_electricity, 0)) +
                   to_number(nvl(moule_exam, 0)) +
                   to_number(nvl(instrument_equi, 0)) +
                   to_number(nvl(use_building, 0)) +
                   to_number(nvl(equi_rental, 0)) +
                   to_number(nvl(faclities_fee, 0)) +
                   to_number(nvl(technology_fee, 0)) +
                   to_number(nvl(office_fee, 0)) +
                   to_number(nvl(enterprice_fee, 0)) +
                   to_number(nvl(school_fee, 0)) +
                   to_number(nvl(abroad_fee, 0)) +
                   to_number(nvl(equi_price, 0)) +
                   to_number(nvl(building_price, 0)) +
                   to_number(nvl(government_funds, 0))) as platin_pay,
               sum(to_number(nvl(equi_price, 0))) as equi_pri_sum
          from rpms_science_activity
         where 1 = 1 ${con2})
]]></property>
