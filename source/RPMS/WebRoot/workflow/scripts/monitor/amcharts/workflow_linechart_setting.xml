<settings>
  <font>Tahoma</font>
  <hide_bullets_count>18</hide_bullets_count>
  <decimals_separator>.</decimals_separator>
  <scientific_min>0.01</scientific_min>
  <background>
    <alpha>90</alpha>
    <border_alpha>10</border_alpha>
  </background>
  <plot_area>
    <margins>
      <left>50</left>
      <right>40</right>
      <bottom>65</bottom>
    </margins>
  </plot_area>
  <grid>
    <x>
      <alpha>10</alpha>
      <approx_count>9</approx_count>
    </x>
    <y_left>
      <alpha>10</alpha>
    </y_left>
    <y_right>
      <enabled>0</enabled>
    </y_right>
  </grid>
  <axes>
    <x>
      <width>1</width>
      <color>0D8ECF</color>
    </x>
    <y_left>
      <width>1</width>
      <color>0D8ECF</color>
    </y_left>
  </axes>
  <values>
    <y_left>
      <min>0</min>
      <max>5</max>
    </y_left>
  </values>
  <indicator>
    <color>0D8ECF</color>
    <x_balloon_text_color>FFFFFF</x_balloon_text_color>
    <line_alpha>50</line_alpha>
    <selection_color>0D8ECF</selection_color>
    <selection_alpha>20</selection_alpha>
  </indicator>
  <zoom_out_button>
    <text_color_hover>FF0F00</text_color_hover>
  </zoom_out_button>
  <help>
    <button>
      <text>help</text>
      <color>FCD202</color>
      <text_color>000000</text_color>
      <text_color_hover>FF0F00</text_color_hover>
    </button>
    <balloon>
      <color>FCD202</color>
      <text_color>000000</text_color>
    </balloon>
  </help>
  <graphs>
    <graph gid="1">
      <title>事故请求</title>
    </graph>
    <graph gid="2">
      <title>问题请求</title>
    </graph>
    <graph gid="3">
      <title>变更请求</title>
    </graph>
    <graph gid="4">
      <title>服务请求</title>
    </graph>
  </graphs>
  <labels>
    <label lid="0">
      <text>流程平均处理时间</text>
      <y>25</y>
      <text_size>13</text_size>
      <align>center</align>
    </label>
  </labels>
  <guides>
    <guide gid="1"/>
  </guides>
</settings>