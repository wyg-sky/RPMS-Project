<settings>
  <font>宋体</font>
  <text_size>12</text_size>
  <decimals_separator>.</decimals_separator>
  <background>
    <alpha>100</alpha>
    <border_color>FFFFFF</border_color>
  </background>
  <plot_area>
    <margins>
      <left>40</left>
      <right>40</right>
      <top>30</top>
      <bottom>50</bottom>
    </margins>
  </plot_area>
  <grid>
    <value>
      <fill_color>000000</fill_color>
      <fill_alpha>15</fill_alpha>
    </value>
  </grid>
  <axes>
    <category>
      <width>1</width>
      <color>E7E7E7</color>
    </category>
    <value>
      <width>1</width>
      <color>E7E7E7</color>
    </value>
  </axes>
  <values>
    <category>
      <color>000000</color>
    </category>
    <value>
      <min>0</min>
      <skip_first>0</skip_first>
    </value>
  </values>
  <balloon>
    <text_color>000000</text_color>
  </balloon>
  <legend>
    <max_columns>2</max_columns>
    <color>000000</color>
    <align>center</align>
  </legend>
  <depth>15</depth>
  <column>
    <width>60</width>
    <hover_brightness>50</hover_brightness>
    <balloon_text>数量: {value}</balloon_text>
    <data_labels>{value}</data_labels>
    <data_labels_text_color>000000</data_labels_text_color>
    <data_labels_text_size>12</data_labels_text_size>
    <data_labels_always_on>1</data_labels_always_on>
    <grow_time>1.5</grow_time>
    <grow_effect>regular</grow_effect>
    <sequenced_grow>1</sequenced_grow>
  </column>
  <graphs>
    <graph gid="0">
      <title>创建</title>
      <color>3399CC</color>
    </graph>
    <graph gid="1">
      <title>关闭</title>
    </graph>
  </graphs>
</settings>